<p><%= notice %></p>

<%= form_with(model: @contact, local: true) do |form| %>

  <div class="contact_name">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class= "contact_mail">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div class = "contact_content">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>

  <%= form.submit %>
<% end %>  